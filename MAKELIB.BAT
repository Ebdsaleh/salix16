@ECHO OFF
REM MAKELIB.BAT
ECHO Building SALIX16.LIB from LIBLIST.TXT FILE
IF NOT EXIST LIBLIST.TXT ECHO ERROR: LIBLIST.TXT Not found! Aborting operation...
IF NOT EXIST LIBLIST.TXT GOT FAIL

IF EXIST BIN\SALIX16.LIB DEL BIN\SALIX16.LIB
TLIB BIN\SALIX16.LIB
IF EXIST BIN\SALIX16.LIB GOTO SUCCESS

:FAIL
ECHO OPERATION WAS ARBORTED!
GOTO END

:SUCCESS
ECHO OPERATION WAS SUCCESSFUL.
GOTO END